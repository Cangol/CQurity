<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/common_btn_enable"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <solid android:color="@color/btn_circle_5"/>
                </shape>
            </item>
            <item>
                <shape android:shape="oval">
                    <solid android:color="@color/btn_circle_pressed"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/btn_circle_5"/>
        </shape>
    </item>
</selector>

